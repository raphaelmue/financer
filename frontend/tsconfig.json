{
    "compilerOptions": {
        "target": "ES6",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "baseUrl": ".",
        "types": ["cypress"],
        "noFallthroughCasesInSwitch": true
    },
    "include": [
        "src/"
    ],
    "exclude": [
        "src/.openapi",
        "test/"
    ]
}
